<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="tcc_launch_sub_task_1" model="sub.task">
        <field name="sub_task_name">Launch The Conversion Cloud.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
    </record>

    <record id="tcc_launch_sub_task_2" model="sub.task">
        <field name="sub_task_name">Set up The Conversion Cloud based on Sales/Client’s specifications.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_tcc_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
    </record>
    <record id="tcc_launch_sub_task_3" model="sub.task">
        <field name="sub_task_name">Deploy The Conversion Cloud apps indicated via Signal.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.tcc_launch_sub_task_2'))]"/>
    </record>

    <record id="tcc_launch_sub_task_4" model="sub.task">
        <field name="sub_task_name">Set up TCC Google Analytics goals.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.tcc_launch_sub_task_2'))]"/>
    </record>

    <record id="tcc_launch_sub_task_5" model="sub.task">
        <field name="sub_task_name">Gain client’s approval on TCC implementation. Set client and apps to Active status
            in the dashboard.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.tcc_launch_sub_task_3'))]"/>
    </record>
    <record id="tcc_launch_sub_task_6" model="sub.task">
        <field name="sub_task_name">Set up Schedule Genie GMB link if SG is among requested apps.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
        <field name="dependency_ids"
               eval="[(4, ref('clx_task_management.tcc_launch_sub_task_2')),(4, ref('clx_task_management.clx_client_launch_sub_task_4'))]"/>
    </record>

    <record id="tcc_launch_sub_task_7" model="sub.task">
        <field name="sub_task_name">Set up sitelink extensions for SG, CM, and/or IC if among requested apps.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.tcc_launch_sub_task_2'))]"/>
    </record>

    <record id="tcc_launch_sub_task_8" model="sub.task">
        <field name="sub_task_name">Review TCC software launch.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.tcc_launch_sub_task_5'))]"/>
    </record>
    <record id="tcc_launch_sub_task_9" model="sub.task">
        <field name="sub_task_name">Verify TCC data is linked within reporting.</field>
        <field name="parent_id" ref="clx_task_management.clx_tcc_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.tcc_launch_sub_task_4'))]"/>
    </record>

</odoo>