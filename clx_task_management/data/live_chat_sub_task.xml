<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!--subtask for live chat Launch -->
    <record id="livechat_sub_task_1" model="sub.task">
        <field name="sub_task_name">Confirm client preferences that might supplement questionnaires. Default launch will be TCC integrated Live Chat campaign.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_live_chat_task"/>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
        <field name="team_id" ref="clx_task_management.clx_sales_team"></field>
    </record>
    <record id="livechat_sub_task_2" model="sub.task">
        <field name="sub_task_name">Set up Live Chat campaign.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_live_chat_task"/>
        <field name="team_id" ref="clx_task_management.clx_live_chat_team"></field>
    </record>
    <record id="livechat_sub_task_3" model="sub.task">
        <field name="sub_task_name">Launch Live Chat campaign.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_live_chat_task"/>
        <field name="team_id" ref="clx_task_management.clx_live_chat_team"></field>
    </record>
    <record id="livechat_sub_task_4" model="sub.task">
        <field name="sub_task_name">Verify Live Chat data is linked within reporting.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_live_chat_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
    </record>
</odoo>
