        <field name="dependency_ids" eval="[(4, ref('clx_task_management.instagram_stories_sub_task_8'))]"/>
    </record>
    <record id="instagram_stories_sub_task_10" model="sub.task">
        <field name="sub_task_name">Verify Instagram Stories data is linked within reporting.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.instagram_stories_sub_task_6'))]"/>
