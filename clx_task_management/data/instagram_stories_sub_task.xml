<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!--subtask for instagram stories Launch -->
    <record id="instagram_stories_sub_task_1" model="sub.task">
        <field name="sub_task_name">Provide any Instagram Stories media preferences that might supplement client questionnaires.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
        <field name="team_id" ref="clx_task_management.clx_sales_team"></field>
    </record>
    <record id="instagram_stories_sub_task_2" model="sub.task">
        <field name="sub_task_name">Audit client questionnaire responses and any campaign preferences.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
    </record>
    <record id="instagram_stories_sub_task_3" model="sub.task">
        <field name="sub_task_name">Set up Instagram Stories campaign.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
    </record>
    <record id="instagram_stories_sub_task_4" model="sub.task">
        <field name="sub_task_name">Create Instagram Stories video.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_cat_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.instagram_stories_sub_task_2'))]"/>
    </record>
    <record id="instagram_stories_sub_task_5" model="sub.task">
        <field name="sub_task_name">Upload Instagram Stories video to Facebook and store Social proofing permalink.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.instagram_stories_sub_task_4'))]"/>
    </record>
    <record id="instagram_stories_sub_task_6" model="sub.task">
        <field name="sub_task_name">Gain client's approval on Instagram Stories media.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.instagram_stories_sub_task_5'))]"/>
    </record>
    <record id="instagram_stories_sub_task_7" model="sub.task">
        <field name="sub_task_name">If eligible to launch early, revise launch day to current day. (SYSTEM)
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_system_team"></field>
    </record>
    <record id="instagram_stories_sub_task_8" model="sub.task">
        <field name="sub_task_name">Launch Instagram Stories media approved by client.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
        <field name="team_id" ref="clx_task_management.clx_ops_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.instagram_stories_sub_task_6'))]"/>
    </record>
    <record id="instagram_stories_sub_task_9" model="sub.task">
        <field name="sub_task_name">Review Instagram Stories campaign launch.
        </field>
        <field name="parent_id" ref="clx_task_management.clx_instagram_stories_task"/>
