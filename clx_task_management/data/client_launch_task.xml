            <field name="team_ids" eval="[(4, ref('clx_task_management.clx_accounting_team'))]"></field>
        </record>
        <record id="clx_client_launch_sub_task_8" model="sub.task">
            <field name="sub_task_name">Verify payment received.</field>
            <field name="parent_id" ref="clx_task_management.clx_client_launch_task"/>
