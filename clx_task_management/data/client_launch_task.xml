<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!--Sub Task For Client Launch-->
    <data noupdate="1">
        <record id="clx_client_launch_sub_task_1" model="sub.task">
            <field name="sub_task_name">Provide client questionnaire answers or confirm questionnaire submitted by
                client.
            </field>
            <field name="parent_id" ref="clx_task_management.clx_client_launch_task"/>
            <field name="team_ids" eval="[(4, ref('clx_task_management.clx_sales_team'))]"></field>
        </record>
        <record id="clx_client_launch_sub_task_2" model="sub.task">
            <field name="sub_task_name">Verify Signal is present on the client's website.</field>
            <field name="parent_id" ref="clx_task_management.clx_client_launch_task"/>
            <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
            <field name="team_ids" eval="[(4, ref('clx_task_management.clx_cs_team'))]"></field>
        </record>

        <record id="clx_client_launch_sub_task_3" model="sub.task">
            <field name="sub_task_name">Verify Google Analytics access (edit or higher) has been provided.</field>
            <field name="parent_id" ref="clx_task_management.clx_client_launch_task"/>
            <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
            <field name="team_ids" eval="[(4, ref('clx_task_management.clx_cs_team'))]"></field>
        </record>

