<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <!--Sequence for create sub task-->
    <record id="sequence_sub_task" model="ir.sequence">
        <field name="name">sub task</field>
        <field name="code">sub.task</field>
        <field name="prefix">SUB/</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <!--Sequence for creating Request form-->
    <record id="sequence_request_form" model="ir.sequence">
        <field name="name">Request form</field>
        <field name="code">request.form</field>
        <field name="prefix">RF/</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
