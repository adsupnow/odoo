        <field name="parent_id" ref="clx_task_management.clx_google_ads_launch_task"/>
    </record>

    <record id="google_ads_launch_sub_task_2" model="sub.task">
        <field name="sub_task_name">Provide any Google Ads campaign preferences that might supplement client questionnaires.</field>
        <field name="parent_id" ref="clx_task_management.clx_google_ads_launch_task"/>
