    <record id="google_ads_launch_sub_task_3" model="sub.task">
        <field name="sub_task_name">Audit client questionnaire responses and any campaign preferences.</field>
        <field name="parent_id" ref="clx_task_management.clx_google_ads_launch_task"/>
        <field name="team_id" ref="clx_task_management.clx_cs_team"></field>
        <field name="dependency_ids" eval="[(4, ref('clx_task_management.clx_client_launch_sub_task_1'))]"/>
    </record>

