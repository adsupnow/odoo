<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="clx_deactivate_finished_subscriptins" model="ir.cron">
        <field name="name">CLX Subscritions: Deactivate finished subscriptions </field>
        <field name="model_id" ref="sale_subscription.model_sale_subscription"/>
        <field name="state">code</field>
        <field name="code">model.deactivate_finished_subscriptins()</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 2:00:00')" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
    </record>
</odoo>