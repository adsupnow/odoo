<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="sale_subscription_view_form_inherit_clx_ratio_invoice" model="ir.ui.view">
        <field name="name">sale.subscription.form.view</field>
        <field name="model">sale.subscription</field>
        <field name="inherit_id" ref="sale_subscription.sale_subscription_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[last()]" position="inside">
                <page string="Co-Op Customers" name="co_op_customers">
                    <field name="co_opp_partner_ids">
                        <tree>
                            <field name="partner_id"/>
                            <field name="ratio"/>
                        </tree>
                    </field>
                </page>
            </xpath>

        </field>
    </record>
</odoo>